<script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.12"></script>
<script>
  //打字座右铭效果
  try {
    var typed = new Typed("#type", {
      strings: ['我们每一个人都是宇航员,都会找到自己的小行星', '频繁记录,只因生活和你太值得❤'],//字符串
      startDelay: 50,//开始的延迟
      typeSpeed: 200,//打字速度
      backSpeed: 100,//回退速度
      loop: true,//是否循环
      showCursor: false,//显示游标
      shuffle: false//是否随机
    });
  } catch (err) {
  }
</script>